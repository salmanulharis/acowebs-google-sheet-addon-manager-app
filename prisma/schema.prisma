generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Clients {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  password   String
  visitCount Int      @default(0)
  website    String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@map("clients")
}
